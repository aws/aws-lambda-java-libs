{
  "timestamp": {
    "$resolver": "timestamp",
    "pattern": {
      "format": "yyyy-MM-dd'T'HH:mm:ss.SSS'Z'",
      "timeZone": "UTC"
    }
  },
  "level": {
    "$resolver": "level",
    "field": "name"
  },
  "message": {
    "$resolver": "message"
  },
  "logger": {
    "$resolver": "logger",
    "field": "name"
  },

  "errorType": {
    "$resolver": "exception",
    "field": "className"
  },
  "errorMessage": {
    "$resolver": "exception",
    "field": "message"
  },
  "stackTrace": {
    "$resolver": "exception",
    "field": "stackTrace"
  },

  "labels": {
    "$resolver": "mdc",
    "flatten": true,
    "stringified": true
  }
}